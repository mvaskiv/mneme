{
  "expo": {
    "name": "Mneme",
    "description": "To Do's and Notes With Smart Lists and Reminders ",
    "slug": "mneme",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.5",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
